#Hello world を表示

print("Hello World")

#実行　ターミナル　python ファイル名.py

#変数

msg = "Hello World"
print("msg")
msg = "Hello Again"
print(msg)

#定数 ：再代入できない 大文字でかく

ADMIN_EMAIL = "taguchi@gmail.com"

#文字列 \n 改行 \t tab

s = "he\nllo wor\tld"

#複数行

html = """<html>
<body></body>
</html>"""
print(s)
print(html)

#整数

i = 10

# 浮動小数点

f = 23.4

#論理値 True or False

flag = True

# + - * / // % **

x = 10
print(x / 3) # 3.333..
print(x // 3) # 3
print(x % 3) # 1 あまり
print(x ** 2) # 100 べき乗

y = 4
# y = y + 12 
y += 12
print(y)  # 16

# and or not
print(True and False) #False
print(True or False) # True
print(not True) #False

# + * 文字列
print("hello " + "world")
print("hello " * 3)

＃5回まで





